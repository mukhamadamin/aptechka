{
  "expo": {
    "name": "HomeAidKit",
    "slug": "HomeAidKit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/asset.png",
    "scheme": "homeaidkit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "icon": "./assets/images/asset.png",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/asset.png",
        "backgroundImage": "./assets/images/asset.png",
        "monochromeImage": "./assets/images/asset.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.HomeAidKit",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/asset.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/asset.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow camera access to scan medicine barcodes."
        }
      ],
      [
        "@bittingz/expo-widgets",
        {
          "android": {
            "src": "./src/widgets/android-plugin",
            "widgets": [
              {
                "name": "MedicinesWidgetProvider",
                "resourceName": "@xml/medicines_widget_info"
              },
              {
                "name": "ShoppingWidgetProvider",
                "resourceName": "@xml/shopping_widget_info"
              }
            ],
            "distPlaceholder": "com.anonymous.HomeAidKit"
          }
        }
      ],
      "./plugins/with-expo-widgets-pinning"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "34a8195c-c769-4ad6-9c71-986e42b6f795"
      }
    },
    "owner": "muhammdamin_2002"
  }
}
